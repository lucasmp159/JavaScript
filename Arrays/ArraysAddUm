var arr = [10,15,17,22,9,4,2,12,16]
let count = 0
for (x in arr){
    if (arr[x] % 2 == 0){
        count += 1
    }
}
console.log(arr)
console.log('A quantidade de números pares é ' + count)


var notas = [5.5, 6, 4.3, 7, 3.2, 8.2]

for (nota in notas){
    if (notas[nota] < 6){
        notas[nota] += 1
    }
}
console.log(notas)
